    "\n",
    "def generateTable(n):\n",
    "    table = \"\"\n",
    "    for i in range(1, 11):\n",
    "        table += f\"{n} X {i} = {n*i}\\n\"\n",
    "    \n",
    "    with open(f\"tables/table_{n}.txt\", \"w\") as f:\n",
    "        f.write(table)\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "for i in range(2, 21):\n",
    "    generateTable(i)"
   ]
